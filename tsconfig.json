{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@Assets/*": [
        "./src/Assets/*"
      ],
      "@Components/*": [
        "src/Components/*"
      ],
      "@Modules/*": [
        "src/Modules/*"
      ],
      "@Storage/*": [
        "src/Storage/*"
      ],
    }
  }
}
